---
import { types } from "../data/types.js";

const { id, title } = Astro.props;
---

<ul class="flex flex-col gap-5 text-[15px] font-semibold">
    {
        types.map(({ id, name }) => {
            return (
                <li>
                    <a href={`/${id}`}>{name}</a>
                </li>
            );
        })
    }
</ul>
